<h1>Listing daily_rankings</h1>

<table>
  <tr>
    <th>Rank</th>
    <th>Team</th>
    <th>Fg</th>
    <th>Ft</th>
    <th>Three pm</th>
    <th>Reb</th>
    <th>Ast</th>
    <th>Stl</th>
    <th>Blk</th>
    <th>To</th>
    <th>Pts</th>
    <th>Tot points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @daily_rankings.each do |daily_ranking| %>
  <tr>
    <td><%= daily_ranking.rank %></td>
    <td><%= daily_ranking.team %></td>
    <td><%= daily_ranking.fg %></td>
    <td><%= daily_ranking.ft %></td>
    <td><%= daily_ranking.three_pm %></td>
    <td><%= daily_ranking.reb %></td>
    <td><%= daily_ranking.ast %></td>
    <td><%= daily_ranking.stl %></td>
    <td><%= daily_ranking.blk %></td>
    <td><%= daily_ranking.to %></td>
    <td><%= daily_ranking.pts %></td>
    <td><%= daily_ranking.tot_points %></td>
    <td><%= link_to 'Show', daily_ranking %></td>
    <td><%= link_to 'Edit', edit_daily_ranking_path(daily_ranking) %></td>
    <td><%= link_to 'Destroy', daily_ranking, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Daily ranking', new_daily_ranking_path %>
